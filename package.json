    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@transferwise/eslint-config": "^8.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-syntax-highlighter": "^15.5.6",
